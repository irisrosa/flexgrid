{
  "name": "flexgrid",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "/build"
  ],
  "scripts": {
    "old:build:babel": "rimraf dist && yarn types && NODE_ENV=es babel src --out-dir \"dist\" --extensions \".ts,.tsx\" --config-file \"./babel.prod.config.js\"",
    "old:build:esbuild": "",
    "build:swc": "yarn cleanup && swc ./src -d build && yarn types",
    "old:build:ts": "rimraf dist & yarn types && tsc -p tsconfig.json",
    "cleanup": "rimraf build",
    "types": "tsc --project tsconfig.json",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "dev": "nodemon --watch src -e \".ts, .tsx\" --exec \"yarn run build:swc && yalc push --changed --no-scripts\"",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "polished": "^4.1.3",
    "ramda": "^0.27.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.15.0",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/react": "^6.3.7",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.19",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^27.0.1",
    "@types/ramda": "^0.27.44",
    "@types/react": "^17.0.18",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.13",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "eslint": "^7.24.0",
    "jest": "^27.0.6",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "styled-components": "^5.3.0",
    "ts-dedent": "^2.2.0",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "typescript": "^4.2.4"
  }
}
