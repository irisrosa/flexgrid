    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.19",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/ramda": "^0.28.20",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.26",
