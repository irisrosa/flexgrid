{
  "name": "flexgrid",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && yarn types && NODE_ENV=es babel src --out-dir \"dist\" --source-maps --extensions \".ts,.tsx\"",
    "types": "tsc --project tsconfig.prod.json",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "dev": "nodemon --watch src -e \".ts, .tsx\" --exec \"yarn run build && yalc push --changed --no-scripts\"",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "polished": "^4.1.3",
    "ramda": "^0.27.1",
    "react": "^17.0.2"
  },
